cmake_minimum_required (VERSION 3.5 FATAL_ERROR)
project (@_CLONE_REPOSITORY_NAME@ LANGUAGES NONE)

include (ExternalProject)

ExternalProject_Add (@_CLONE_REPOSITORY_NAME@
    PREFIX "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/@_CLONE_REPOSITORY_NAME@"
    TMP_DIR "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/.cmake/tmp"
    STAMP_DIR "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/.cmake/stamp"
    DOWNLOAD_DIR "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/@_CLONE_REPOSITORY_NAME@"
    GIT_REPOSITORY @_CLONE_REPOSITORY_GIT_REPOSITORY@
    GIT_TAG @_CLONE_REPOSITORY_GIT_TAG@
    HG_REPOSITORY @_CLONE_REPOSITORY_HG_REPOSITORY@
    HG_TAG @_CLONE_REPOSITORY_HG_TAG@
    UPDATE_DISCONNECTED 1
    SOURCE_DIR "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/@_CLONE_REPOSITORY_NAME@"
    CONFIGURE_COMMAND ""
    BINARY_DIR "@_CLONE_REPOSITORY_EXTERNAL_PROJECT_DIR@/@_CLONE_REPOSITORY_NAME@"
    BUILD_COMMAND ""
    INSTALL_DIR ""
    INSTALL_COMMAND ""
)
